<!DOCTYPE html>
<html>
    <head>
        <title>插袋大学招聘网后台 </title>
        <%= csrf_meta_tags %>

        <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
        <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    </head>

    <body>

        <div class="container-fluid">
            <%= render "common/navbar" %>
            <%= render "common/flashes" %>
            <div class="row">
              <div class="col-md-2">
                <ul class="nav nav-pills nav-stacked" style="max-width: 120px; padding-top:2px;">
                  <li class="active"><%= link_to("后台职缺列表", admin_jobs_path ) %> </li>

                </ul>
              </div>

              <div class="col-md-10">
                <%= yield %>
              </div>
           </div>

           <%= render "common/footer" %>
        </div>

    </body>
</html>
